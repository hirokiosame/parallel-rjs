{
  "name": "parallel-rjs",
  "version": "1.0.0",
  "description": "Build rjs modules in parallel",
  "main": "dist/index.js",
  "scripts": {
    "build": "node_modules/babel-cli/bin/babel.js ./src -s inline --out-dir ./dist",
    "watch": "node_modules/babel-cli/bin/babel.js ./src -s inline --out-dir ./dist --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Hiroki Osame",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "source-map-support": "^0.4.0"
  },
  "dependencies": {
    "requirejs": "^2.2.0",
    "uglify-js": "^2.6.2"
  }
}
